Program Hello;

var int x;
var int y;
var int z;
var int w;
var int[] arr;
var int i;

main () {
  x = 4;
  z = 2;
  y = 1;
  w = 0;
  i = 0;
  if (x * y < z) {
    print(x);
  } else if (y * x / z > w) {
    print(w);
  } else if (x < z) {
    print(z, w);
  } else {
    print(y);
  }

  while (x < 10) {
    y = x * w * z;
    x = x + 1;
  }

  for (i in arr) {
    print(i);
  }
}

